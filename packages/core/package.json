{
  "name": "@bee/core",
  "version": "0.0.1",
  "description": "mini core",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "release": "npm run build && dsc publish --pnpm -t"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./plugins": {
      "types": "./src/plugins/index.ts",
      "import": "./src/plugins/index.ts"
    },
    "./package.json": "./package.json",
    "./*": "./src/*"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "daysnap",
    "bee",
    "wechat",
    "miniprogram",
    "core",
    "mini"
  ],
  "author": "woshiajuana",
  "license": "MIT",
  "dependencies": {
    "@daysnap/types": "^0.0.5",
    "@daysnap/utils": "^0.0.83",
    "miniprogram-api-typings": "^3.12.2"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0"
  }
}
